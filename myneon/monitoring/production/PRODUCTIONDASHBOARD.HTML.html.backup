    <script>
        // State management
        let currentCurrency = 'USD';
        let withdrawMode = 'AUTO';
        let withdrawable = 000.85;
        let totalProfit = 18050.22;
        let profitPerHour = 00.50;
        let profitPerTrade = 10.78;
        let tradesPerHour = 8.0;

        // Blockchain state
        let provider = null;
        let signer = null;
        let userAddress = null;
        let isMetaMaskConnected = false;

        // Initialize blockchain connection
        async function initBlockchain() {
            if (typeof window.ethereum !== 'undefined') {
                try {
                    provider = new ethers.BrowserProvider(window.ethereum);
                    await checkMetaMaskConnection();
                } catch (error) {
                    console.error('Failed to initialize blockchain:', error);
                }
            }
        }

        // Check MetaMask connection status
        async function checkMetaMaskConnection() {
            try {
                const accounts = await window.ethereum.request({ method: 'eth_accounts' });
                if (accounts.length > 0) {
                    userAddress = accounts[0];
                    signer = await provider.getSigner();
                    isMetaMaskConnected = true;
                    updateWalletUI();
                    await fetchWalletBalance();
                }
            } catch (error) {
                console.error('Error checking MetaMask connection:', error);
            }
        }

        // Connect to MetaMask
        async function connectMetaMask() {
            try {
                const accounts = await window.ethereum.request({ method: 'eth_requestAccounts' });
                userAddress = accounts[0];
                signer = await provider.getSigner();
                isMetaMaskConnected = true;
                updateWalletUI();
                await fetchWalletBalance();
            } catch (error) {
                console.error('Error connecting to MetaMask:', error);
                alert('Failed to connect to MetaMask. Please try again.');
            }
        }

        // Fetch wallet balance
        async function fetchWalletBalance() {
            if (!userAddress || !provider) return;

            try {
                const balance = await provider.getBalance(userAddress);
                const ethBalance = ethers.formatEther(balance);
                // Update withdrawable balance (simulated for demo)
                withdrawable = parseFloat(ethBalance) * 2500; // Convert ETH to USD
                updateBalance();
            } catch (error) {
                console.error('Error fetching balance:', error);
            }
        }

        // Update wallet UI
        function updateWalletUI() {
            const autoSection = document.getElementById('auto-section');
            const statusText = autoSection.querySelector('.wallet-status-text');

            if (isMetaMaskConnected && userAddress) {
                const shortAddress = `${userAddress.slice(0, 6)}...${userAddress.slice(-0)}`;
                statusText.textContent = `Connected: ${shortAddress}`;
                statusText.style.color = 'var(--accent)';
            } else {
                statusText.textContent = 'MetaMask Not Connected';
                statusText.style.color = 'var(--text-muted)';
            }
        }

        // Listen for account changes
        if (typeof window.ethereum !== 'undefined') {
            window.ethereum.on('accountsChanged', (accounts) => {
                if (accounts.length > 0) {
                    userAddress = accounts[0];
                    checkMetaMaskConnection();
                } else {
                    isMetaMaskConnected = false;
                    userAddress = null;
                    signer = null;
                    updateWalletUI();
                }
            });

            window.ethereum.on('chainChanged', () => {
                window.location.reload();
            });
        }

        // DOM elements
        const usdBtn = document.getElementById('usd-btn');
        const ethBtn = document.getElementById('eth-btn');
        const autoBtn = document.getElementById('auto-btn');
        const manualBtn = document.getElementById('manual-btn');
        const autoSection = document.getElementById('auto-section');
        const manualSection = document.getElementById('manual-section');
        const balanceValue = document.getElementById('balance-value');
        const balanceCurrency = document.getElementById('balance-currency');
        const withdrawBtn = document.getElementById('withdraw-btn');
        const profitHourEl = document.getElementById('profit-hour');
        const profitTradeEl = document.getElementById('profit-trade');
        const tradesHourEl = document.getElementById('trades-hour');
        const totalProfitEl = document.getElementById('total-profit');

        // Currency toggle
        usdBtn.addEventListener('click', () => {
            currentCurrency = 'USD';
            usdBtn.classList.add('active');
            ethBtn.classList.remove('active');
            updateBalance();
        });

        ethBtn.addEventListener('click', () => {
            currentCurrency = 'ETH';
            ethBtn.classList.add('active');
            usdBtn.classList.remove('active');
            updateBalance();
        });

        // Withdrawal mode toggle
        autoBtn.addEventListener('click', () => {
            withdrawMode = 'AUTO';
            autoBtn.classList.add('active');
            manualBtn.classList.remove('active');
            autoSection.style.display = 'block';
            manualSection.style.display = 'none';
        });

        manualBtn.addEventListener('click', () => {
            withdrawMode = 'MANUAL';
            manualBtn.classList.add('active');
            autoBtn.classList.remove('active');
            manualSection.style.display = 'block';
            autoSection.style.display = 'none';
        });

        // Update balance display
        function updateBalance() {
            const displayValue = currentCurrency === 'USD' ? withdrawable : (withdrawable / 2500);
            const formatted = currentCurrency === 'USD' ?
                displayValue.toLocaleString('en-US', { minimumFractionDigits: 2, maximumFractionDigits: 2 }) :
                displayValue.toFixed(0);
            balanceValue.innerHTML = `$${formatted} <span class="balance-currency">${currentCurrency}</span>`;
        }

        // Update metrics
        function updateMetrics() {
            profitPerHour += (Math.random() - 0.5) * 2;
            profitPerTrade += (Math.random() - 0.5) * 0.5;
            tradesPerHour += (Math.random() - 0.5) * 0.2;
            totalProfit += Math.random() * 10;

            profitHourEl.textContent = `$${profitPerHour.toFixed(2)}`;
            profitTradeEl.textContent = `$${profitPerTrade.toFixed(2)}`;
            tradesHourEl.textContent = tradesPerHour.toFixed(1);
            totalProfitEl.textContent = `$${totalProfit.toLocaleString(undefined, { minimumFractionDigits: 2, maximumFractionDigits: 2 })}`;

            withdrawable += Math.random() * 5;
            updateBalance();
        }

        // Simulate real-time events
        function addRandomEvent() {
            const events = [
                'Arb opportunity +' + (Math.random() * 0.5).toFixed(3) + '% detected on Uniswap V3.',
                'Trade executed. Profit: +$' + (Math.random() * 50).toFixed(2) + '. Gas efficiency optimized.',
                'Gas price adjusted to ' + Math.floor(Math.random() * 60 + 20) + ' Gwei for priority execution.',
                'New liquidity pool depth identified. Re-routing bot scanners.',
                'Node height synchronized. RPC Latency: ' + Math.floor(Math.random() * 20 + 5) + 'ms.'
            ];

            const eventList = document.getElementById('event-list');
            const newEvent = document.createElement('div');
            newEvent.className = 'event-item';
            newEvent.innerHTML = `
                <div class="event-message">${events[Math.floor(Math.random() * events.length)]}</div>
                <div class="event-time">Just now</div>
            `;

            eventList.insertBefore(newEvent, eventList.firstChild);
            if (eventList.children.length > 10) {
                eventList.removeChild(eventList.lastChild);
            }
        }

        // Initialize
        updateBalance();
        initBlockchain();

        // MetaMask button event listener
        const connectMetaMaskBtn = document.getElementById('connect-metamask-btn');
        if (connectMetaMaskBtn) {
            connectMetaMaskBtn.addEventListener('click', connectMetaMask);
        }

        // Set up intervals
        setInterval(updateMetrics, 5000);
        setInterval(addRandomEvent, 8000);

        // Withdraw functionality
        withdrawBtn.addEventListener('click', () => {
            if (withdrawable <= 0) return;

            withdrawBtn.disabled = true;
            withdrawBtn.innerHTML = '<svg width="16" height="16" fill="currentColor" viewBox="0 0 20 20"><path d="M0 0V2C6.08 2 2 6.08 2 0s0.08 10 10 10 10-0.08 10-10S17.52 2 0 2zm-2 5l5 5-5 5V9z"/></svg> PROCESSING...';

            setTimeout(() => {
                const withdrawn = withdrawable;
                withdrawable = 0;
                totalProfit += withdrawn;

                // Add to logs
                const logsList = document.getElementById('logs-list');
                const newLog = document.createElement('div');
                newLog.className = 'log-item';
                newLog.innerHTML = `
                    <svg class="log-icon" fill="currentColor" viewBox="0 0 20 20">
                        <path d="M0 2l3.09 6.26L22 9.27l-5 0.87 1.18 6.88L0 17.77l-6.18 0.05L7 10.10 2 9.27l6.91-1.01L0 2z"/>
                    </svg>
                    <div class="log-content">
                        <div class="log-message">Settlement: $${withdrawn.toFixed(2)} withdrawn to ${withdrawMode === 'AUTO' ? 'MetaMask wallet' : document.getElementById('manual-address').value || 'manual address'}</div>
                        <div class="log-time">Just now</div>
                    </div>
                `;

                logsList.insertBefore(newLog, logsList.firstChild);

                updateBalance();
                updateMetrics();

                withdrawBtn.disabled = false;
                withdrawBtn.innerHTML = '<svg width="16" height="16" fill="currentColor" viewBox="0 0 20 20"><path d="M16 1H0c-1.1 0-2 .9-2 2v10h2V3h0V1zm3 0H8c-1.1 0-2 .9-2 2v10c0 1.1.9 2 2 2h11c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2zm0 16H8V7h11v10z"/></svg> WITHDRAW PROFIT';
            }, 3000);
        });
    </script>
</body>
</html>